{
  "expo": {
    "name": "LarvaEye",
    "slug": "larvaeye",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/prawn.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow the application to access your camera",
          "microphonePermission": "Allow the application to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos and sends them online for image processing purposes."
        }
      ],
      "expo-secure-store"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#EFF4FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "favicon": "./assets/prawn.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/prawn.png",
        "backgroundColor": "#EFF4FF"
      },
      "package": "com.sunnystartup.appname",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/prawn.png"
    },
    "extra": {
      "eas": {
        "projectId": "b517850a-0a34-4a40-b751-c61c87b5a954"
      }
    },
    "owner": "ren_fuse"
  }
}
