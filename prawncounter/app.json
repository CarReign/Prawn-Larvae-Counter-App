{
  "expo": {
    "name": "Prawn Larvae Counter",
    "slug": "prawncounter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/prawn.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#BAD8F2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/prawn.png",
        "backgroundColor": "#BAD8F2"
      },
      "package": "com.companyname.appname"
    },
    "web": {
      "favicon": "./assets/prawn.png"
    }
  }
}